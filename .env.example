# ==============================================================================
# deep-label Environment Variables
# ==============================================================================
# Copy this file to .env and fill in your values.

# ------------------------------------------------------------------------------
# REQUIRED
# ------------------------------------------------------------------------------

# Google Generative AI API key (required for detection)
GOOGLE_GENERATIVE_AI_API_KEY=your_api_key_here

# ------------------------------------------------------------------------------
# MODEL CONFIGURATION
# ------------------------------------------------------------------------------

# Model for object detection and verification
MODEL_NAME=gemini-3-pro-preview

# Model for generating accessibility descriptions (can be same or different)
DESCRIPTION_MODEL_NAME=gemini-3-pro-preview

# ------------------------------------------------------------------------------
# DETECTION SETTINGS
# ------------------------------------------------------------------------------

# Maximum number of object kinds to discover (1-200)
MAX_KINDS=50

# Number of verification rounds per kind (0-10, 0=skip verification)
VERIFY_ROUNDS=2

# Enable tiled detection for objects above this count threshold (0=disable tiling)
# Note: tiny/small objects always use tiling regardless of count
TILE_THRESHOLD=12

# Maximum concurrent API calls (1-20)
CONCURRENCY=6

# Enable multi-scale discovery (analyzes quadrants to find small details)
# Set to 0 or false to disable
MULTI_SCALE=true

# ------------------------------------------------------------------------------
# CUTOUT SETTINGS
# ------------------------------------------------------------------------------

# Enable cutout generation (set to 0 or false to disable)
CUTOUTS=true

# Cutout image format: webp or png
CUTOUTS_FORMAT=webp

# Thumbnail size in pixels (0=no thumbnails)
CUTOUTS_THUMB_SIZE=256

# Maximum number of cutouts to generate (default: unlimited)
# CUTOUTS_MAX=100

# Concurrent cutout generation (1-64)
CUTOUTS_CONCURRENCY=8

# Padding around cutouts as decimal (0.10 = 10% padding on each side)
CUTOUTS_PADDING=0.10

# ------------------------------------------------------------------------------
# DEBUG OPTIONS
# ------------------------------------------------------------------------------

# Show detailed error information including stack traces
DEBUG_ERRORS=0

# Show AI pool debug information (request queuing, timing)
DEBUG_POOL=0
